options:
	
	#Variables:
		# %{cheat}% - Mostrar el nombre del hack detectado.
	
	#Messages:
	Prefix: &2&lNova&4&lDetector
	MPrefix: &2&lN&4&lD
	TMPrefix: &2&lN&4&lD&c&lT&4&l
	SubPrefix: &c&lAntiCheat
	Version: Build 11.1
	AutoKickA: &2&lAntiCheat &7» &f%{cheat}%
	AutoKickV: &2&lAntiCheat &7» &f%{cheat}%
	AutoKickS: &2&lAntiCheat &7» &f%{cheat}%
	AutoKickP: &2&lAntiCheat &7» &f%{cheat}%
	KickForCommand1: &2&lAntiCheat &7» &f%arg 3%.
	KickForCommand2: &2&lAntiCheat &7» &fSin Motivo.
	NoPermission: &fUnknown command. Type "/help" for help.
	AlertP: &e&l%player% &ffallo
	AlertS: &e&l%shooter% &ffallo
	AlertV: &e&l%victim% &ffallo
	AlertA: &e&l%attacker% &ffallo
	AlertLP: &e&l%loop-player% &ffallo
	AlertLP1: &e&l%loop-player-1% &ffallo
	KickP: &e&l%player% &ffue removido por &4%{cheat}%
	KickS: &e&l%shooter% &ffue removido por &4%{cheat}%
	KickV: &e&l%victim% &ffue removido por &4%{cheat}%
	KickA: &e&l%attacker% &ffue removido por &4%{cheat}%
	KickLP: &e&l%loop-player% &ffue removido por &4%{cheat}%
	KickLP1: &e&l%loop-player-1% &ffue removido por &4%{cheat}%
	MessageAllChat1P: &8&m-------------------------------------------
	MessageAllChat2P: &8» &c%player% &fa sido kickeado.
	MessageAllChat3P: &c&lMotivo &8» &cUso de Hacks &8(&c%{cheat}%&8)
	MessageAllChat4P: &8&m-------------------------------------------
	MessageAllChat1A: &8&m-------------------------------------------
	MessageAllChat2A: &8» &c%attacker% &fa sido kickeado.
	MessageAllChat3A: &c&lMotivo &8» &cUso de Hacks &8(&c%{cheat}%&8)
	MessageAllChat4A: &8&m-------------------------------------------
	MessageAllChat1S: &8&m-------------------------------------------
	MessageAllChat2S: &8» &c%shooter% &fa sido kickeado.
	MessageAllChat3S: &c&lMotivo &8» &cUso de Hacks &8(&c%{cheat}%&8)
	MessageAllChat4S: &8&m-------------------------------------------
	MessageAllChat1V: &8&m-------------------------------------------
	MessageAllChat2V: &8» &c%victim% &fa sido kickeado.
	MessageAllChat3V: &c&lMotivo &8» &cUso de Hacks &8(&c%{cheat}%&8)
	MessageAllChat4V: &8&m-------------------------------------------
	MessageAllChat1LP: &8&m-------------------------------------------
	MessageAllChat2LP: &8» &c%loop-player% &fa sido kickeado.
	MessageAllChat3LP: &c&lMotivo &8» &cUso de Hacks &8(&c%{cheat}%&8)
	MessageAllChat4LP: &8&m-------------------------------------------
	
	# Checks:
	# HitReach
	Reach-Distance: 5
	Reach-Cancel: true
	
	# KillAura
	Killaura-Distance: 3.8
	Killaura-CPS: 14
	Killaura-Ticks: 2.1
	Killaura-Cancel: true
	
	# Anti-KB | Velocity
	AntiKB-Distance: 1.01
	AntiKB-Cancel: true
	
	# FastBow
	FastBow-Shoots: 6
	FastBow-Ticks: 1.8
	
	# Jesus
	Jesus-Distance: 0.75
	Jesus-Cancel: true
	
	# FastEat
	FastEat-Second: 1
	FastEat-Cancel: true
	
	# FastPlace
	FastPlace-Blocks: 6
	FastPlace-Reset: 0.8
	FastPlace-Cancel: true

#
# End of Config
#


#
#SKRIPT
#Please not touch!
#

on skript start:

	# KillAura
	if {KillauraCheckFFB:N} is not set:
		set {KillauraCheckFFB:N} to true
	if {KillAuraCheck} is not set:
		set {KillAuraCheck} to "&eActivado"

	# AntiKB
	if {AntiKBCheckFFB:N} is not set:
		set {AntiKBCheckFFB:N} to true
	if {AntiKBCheck} is not set:
		set {AntiKBCheck} to "&eActivado"

	# Reach
	if {AutoActiveKill::N} is not set:
		set {AutoActiveKill::N} to true
	if {ReachCheckFFB:N} is not set:
		set {ReachCheckFFB:N} to true
	if {ReachCheck} is not set:
		set {ReachCheck} to "&eActivado"

	# FastBow
	if {FastBowCheckFFB:N} is not set:
		set {FastBowCheckFFB:N} to true
	if {FastBowCheck} is not set:
		set {FastBowCheck} to "&eActivado"

	# FastEat
	if {FastEatCheckFFB:N} is not set:
		set {FastEatCheckFFB:N} to true
	if {FastEat:Check} is not set:
		set {FastEat:Check} to "&eActivado"

	# Jesus	
	if {JesusCheckFFB:N} is not set:
		set {JesusCheckFFB:N} to true
	if {Jesus::Check} is not set:
		set {Jesus::Check} to "&eActivado"

	# FastPlace
	if {FastPlaceCheckFFB:N} is not set:
		set {FastPlaceCheckFFB:N} to true
	if {FastPlaceCheck} is not set:
		set {FastPlaceCheck} to "&eActivado"
		
	message "" to console
	set {TestMode} to "&eDesactivado"
	set {TestMode::N} to false
	message "&2&lNova&4&lDetector &bService &ea sido activado (Version: Build 11.1)" to console
	message "&2&lNova&4&lDetector &bService &aServer Version &e(Auto-Detectado): &b%minecraft version%" to console
	message "&2&lNova&4&lDetector &bService &aBukkit Version &e(Auto-Detectado): &b%bukkit version%" to console
	message "&2&lNova&4&lDetector &bService &aSkript Version &e(Auto-Detectado): &b%skript version%" to console
	message "&2&lNova&4&lDetector &a&lKillaura  &e&lActivado &8| &fEstado: %{KillAuraCheck}%" to console
	message "&2&lNova&4&lDetector &a&lAntiKB    &e&lActivado &8| &fEstado: %{AntiKBCheck}%" to console
	message "&2&lNova&4&lDetector &a&lReach     &e&lActivado &8| &fEstado: %{ReachCheck}%" to console
	message "&2&lNova&4&lDetector &a&lFastBow   &e&lActivado &8| &fEstado: %{FastBowCheck}%" to console
	message "&2&lNova&4&lDetector &a&lFastEat   &e&lActivado &8| &fEstado: %{FastEat:Check}%" to console	
	message "&2&lNova&4&lDetector &a&lJesus     &e&lActivado &8| &fEstado: %{Jesus::Check}%" to console
	message "&2&lNova&4&lDetector &a&lFastPlace &e&lActivado &8| &fEstado: %{FastPlaceCheck}%" to console
	message "&2&lNova&4&lDetector &a&lGracias por usar &2&lNova&4&lDetector" to console
	message "" to console

on skript stop:
	message "" to console
	set {TestMode} to "&eDesactivado"
	set {TestMode::N} to false
	message "&2&lNova&4&lDetector &bService &ea sido desactivado (Version: Build 11.1)" to console
	message "&2&lNova&4&lDetector &bService &aServer Version &e(Auto-Detectado): &b%minecraft version%" to console
	message "&2&lNova&4&lDetector &bService &aBukkit Version &e(Auto-Detectado): &b%bukkit version%" to console
	message "&2&lNova&4&lDetector &bService &aSkript Version &e(Auto-Detectado): &b%skript version%" to console
	message "&2&lNova&4&lDetector &a&lKillaura  &e&lDesactivado &8| &fEstado: %{KillAuraCheck}%" to console
	message "&2&lNova&4&lDetector &a&lAntiKB    &e&lDesactivado &8| &fEstado: %{AntiKBCheck}%" to console
	message "&2&lNova&4&lDetector &a&lHitReach  &e&lDesactivado &8| &fEstado: %{ReachCheck}%" to console
	message "&2&lNova&4&lDetector &a&lFastBow   &e&lDesactivado &8| &fEstado: %{FastBowCheck}%" to console
	message "&2&lNova&4&lDetector &a&lFastEat   &e&lDesactivado &8| &fEstado: %{FastEat:Check}%" to console	
	message "&2&lNova&4&lDetector &a&lJesus     &e&lDesactivado &8| &fEstado: %{Jesus::Check}%" to console
	message "&2&lNova&4&lDetector &a&lFastPlace &e&lDesactivado &8| &fEstado: %{FastPlaceCheck}%" to console
	message "&2&lNova&4&lDetector &a&lGracias por usar &2&lNova&4&lDetector" to console
	message "" to console

on join:
	player has permission "Nova.use":
		wait 1 second
		set {_update} to text from url "https://pastebin.com/raw/NVFaFeqD"
		{_update} is not "{@Version}":
			loop all players:
				if loop-player has permission "Nova.use":
					message "{@Prefix} &7» &aNueva actualización" to loop-player
					message "{@Prefix} &7» &aVersion Instalada: &4{@Version}" to loop-player
					message "{@Prefix} &7» &aVersion Reciente: &2%{_update}%" to loop-player
				message "{@Prefix} &7» &aNueva actualización" to console
				message "{@Prefix} &7» &aVersion Instalada: &4{@Version}" to console
				message "{@Prefix} &7» &aVersion Reciente: &2%{_update}%" to console
				message "{@Prefix} &7» &aActualiza en: &bhttps://www.spigotmc.org/resources/novadetector-anticheat-skript.74796/" to console

on death:
	if {Anti-KB.CancelEvent.%player%} is true:
		set {Anti-KB.CancelEvent.%player%} to false
		wait 5 second
		set {Anti-KB.CancelEvent.%player%} to true
	if {Anti-Clcik.CancelEvent.%player%} is true:
		set {Anti-Clcik.CancelEvent.%player%} to false
		wait 5 second
		set {Anti-Clcik.CancelEvent.%player%} to true

on join:
	loop 5 times:
		message "" to player
	if {VLBreaked::N} is not set:
		set {VLBreaked::N} to 0
	if {VLKicked::N} is not set:
		set {VLKicked::N} to 0
	loop all players:
		loop-player has permission "Nova.use" or "Nova.Staff":
			message "" 
			message "{@Prefix} &7» &e&lBienvenido al Servidor"
			message "{@Prefix} &7» &aHubo &b%{VLBreaked::N}% &adetectados en total."
			message "{@Prefix} &7» &aHubo &b%{VLKicked::N}% &aancionados en total."
			message "" 
	if {Anti-KB.CancelEvent.%player%} is true:
		set {Anti-KB.CancelEvent.%player%} to false
		wait 5 second
		set {Anti-KB.CancelEvent.%player%} to true
	if {Anti-Clcik.CancelEvent.%player%} is true:
		set {Anti-Clcik.CancelEvent.%player%} to false
		wait 5 second
		set {Anti-Clcik.CancelEvent.%player%} to true

command /Nova [<text>]:
	permission: Nova.use
	permission message: {@NoPermission}
	aliases: /NovaDetector, /AntiCheat
	usage: /Nova [<text>]
	trigger:
		if arg-1 is not set:
			if sender has permission "Nova.use":
				send "" to sender
				send "&4&l================&b&l[1/2]&4&l====================" to sender
				send "{@Prefix} &e&lComandos:" to sender		  
				send "" to sender
				send "&4&l/Nova &2&lVersion" to sender		  
				send "&4&l/Nova &2&lHacksDetect" to sender
				send "&4&l/Nova &2&lhelp [1/2]" to sender
				send "&4&l/Nova &2&lInfo" to sender
				send "&2&l================&b&l[1/2]&2&l==================" to sender
				stop
		if arg-1 is "help 2":
			if sender has permission "Nova.use":
				send "" to sender
				send "&4&l================&b&l[2/2]&4&l====================" to sender
				send "{@Prefix} &e&lComandos:" to sender		  
				send "" to sender
				send "&4&l/NovaKick &2&l[Jugador] &b&l[Motivo]" to sender
				send "&4&l/Nova &2&lGui" to sender
				send "&4&l/NovaClear &2&l[Jugador] &b&l[Check]" to sender
				send "&4&l/Nova &2&lReload" to sender
				send "&2&l================&b&l[2/2]&2&l====================" to sender
				stop
		if arg-1 is "Version":
			if sender has permission "Nova.use":
				send "&4&l========================================" to sender
				send "&4&lVersion:" to sender
				send "&2&lBuild 11.1" to sender				  	  
				send "&2&l========================================" to sender
				stop
		if arg-1 is "HacksDetect":
			if sender has permission "Nova.use":
				send "&4&l========================================" to sender
				send "&b&lHacksDetect:" to sender			  
				send "" to sender									  		  
				send "&2&lKillaura &a&l(Done)" to sender
				send "&2&lAntiKB &a&l(Done)" to sender
				send "&2&lHitReach &a&l(Done)" to sender
				send "&2&lFastBow &a&l(Done)" to sender
				send "&2&lJesus &a&l(Done)" to sender
				send "&2&lFastEat &a&l(Done)" to sender
				send "&2&lFastPlace &a&l(Done)" to sender
				send "" to sender		  
				send "&2&l========================================" to sender
				stop
		if arg-1 is "help":
			if sender has permission "Nova.use":
				send "" to sender
				send "&4&l================&b&l[1/2]&4&l====================" to sender
				send "{@Prefix} &b&lComandos:" to sender		  
				send "" to sender
				send "&4&l/Nova &2&lVersion" to sender		  
				send "&4&l/Nova &2&lHacksDetect" to sender
				send "&4&l/Nova &2&lhelp [1/2]" to sender
				send "&4&l/Nova &2&lInfo" to sender
				send "&2&l================&b&l[1/2]&2&l====================" to sender
				stop
		if arg-1 is "Info":
			if sender has permission "Nova.use":
				send "&4&l========================================" to sender
				send "{@Prefix}:" to sender				
				send "" to sender		 				          
				send "&4&lCreator: &2&lNovaCraft254" to sender	   
				send "&4&lSubCreator: &2&lAlexMod" to sender
				send "" to sender
				send "&4&lStart of the project: &2&l29/1/2020" to sender				
				send "&2&l========================================" to sender
				stop
		if arg-1 is "Reload":
			if sender has permission "Nova.use":
				message "{@Prefix} &7» &aRecargando..." to sender
				execute console command "/sk reload %script%"
				message "{@Prefix} &7» &aRecargado!" to sender
				stop
		if arg-1 is "Gui":
			if player has permission "Nova.use":
				message "{@Prefix} &7» &bAbriendo Menu..." to sender
				wait 0.1 seconds
				open chest with 4 rows named "{@Prefix}" to player
				wait 0.1 seconds
				format slot 4 of player with Block of Redstone named "&c&lCerrar Menu" to close
				format slot 20 of player with chest named "&e&lChecks" to close then run [execute player command "/ChecksMenu"]
				format slot 24 of player with paper named "&e&lTestMode" with lore "&bEstado: %{TestMode}%" to close then run [execute player command "/novatoggle testmode"]
				stop

command /ChecksMenu:
	permission: Nova.use
	permission message: {@NoPermission}
	trigger:
		wait 0.1 seconds
		open chest with 5 rows named "{@Prefix}> &eChecks" to player
		wait 0.1 seconds
		format slot 4 of player with Block of Redstone named "&c&lCerrar Menu" to close
		format slot 0 of player with Green Wool named "&2&lActivar todos los Checks" to close then run [execute player command "/novatoggle all enable"]
		format slot 8 of player with Red Wool named "&c&lDesactivar todos los Checks" to close then run [execute player command "/novatoggle all disable"]
		format slot 9 of player with Diamond Sword named "&a&lCombat &e&lBlocks" to close
		format slot 10 of player with Paper named "&a&lHitReach &e&lCheck" with lore "&bEstado: %{ReachCheck}%" to close then run [execute player command "/novatoggle hitreach"]
		format slot 11 of player with Paper named "&a&lKillAura &e&lCheck" with lore "&bEstado: %{KillAuraCheck}%" to close then run [execute player command "/novatoggle killaura"]
		format slot 12 of player with Paper named "&a&lFastBow &e&lCheck" with lore "&bEstado: %{FastBowCheck}%" to close then run [execute player command "/novatoggle fastbow"]
		format slot 18 of player with Diamond Boots named "&a&lMovement &e&lBlocks:" to close
		format slot 19 of player with Paper named "&a&lAntiKnockback &e&lCheck" with lore "&bEstado: %{AntiKBCheck}%" to close then run [execute player command "/novatoggle antikb"]
		format slot 20 of player with paper named "&a&lJesus &e&lCheck" with lore "&bEstado: %{Jesus::Check}%" to close then run [execute player command "/novatoggle jesus"]
		format slot 27 of player with Diamond Helmet named "&a&lPlayer &e&lBlocks" to close
		format slot 28 of player with paper named "&a&lFastEat &e&lCheck" with lore "&bEstado: %{FastEat:Check}%" to close then run [execute player command "/novatoggle fasteat"]
		format slot 36 of player with Grass named "&a&lWorld &e&lBlocks" to close
		format slot 37 of player with paper named "&a&lFastPlace &e&lCheck" with lore "&bEstado: %{FastPlaceCheck}%" to close then run [execute player command "/novatoggle fastplace"]
		stop
	
command /novatoggle [<text>] [<text>]:
	permission: Nova.use
	permission message: {@NoPermission}
	trigger:
		if arg-1 is "all":
			if arg-2 is "enable":
				set {ReachCheckFFB:N} to true
				set {AutoActiveKill::N} to true
				set {ReachCheck} to "&eActivado"
				set {KillAuraCheckFFB:N} to true
				set {KillauraCheck} to "&eActivado"
				set {FastBowCheckFFB:N} to true
				set {FastBowCheck} to "&eActivado"
				set {AntiKBCheckFFB:N} to true
				set {AntiKBCheck} to "&eActivado"
				set {JesusCheckFFB:N} to true
				set {Jesus::Check} to "&eActivado"
				set {FastEatCheckFFB:N} to true
				set {FastEat:Check} to "&eActivado"
				set {FastPlaceCheckFFB:N} to true
				set {FastPlaceCheck} to "&eActivado"
				wait 0.1 seconds
				execute player command "/ChecksMenu"
				loop all players:
					if loop-player has permission "Nova.use":
						message "{@MPrefix} &7» &aTodos los checks fueron activados por &e%sender%" to loop-player	
						stop
		if arg-1 is "all":
			if arg-2 is "disable":
				set {ReachCheckFFB:N} to false
				set {AutoActiveKill::N} to false
				set {ReachCheck} to "&eDesactivado"
				set {KillAuraCheckFFB:N} to false
				set {KillauraCheck} to "&eDesactivado"
				set {FastBowCheckFFB:N} to false
				set {FastBowCheck} to "&eDesactivado"
				set {AntiKBCheckFFB:N} to false
				set {AntiKBCheck} to "&eDesactivado"
				set {JesusCheckFFB:N} to false
				set {Jesus::Check} to "&eDesactivado"
				set {FastEatCheckFFB:N} to false
				set {FastEat:Check} to "&eDesactivado"
				set {FastPlaceCheckFFB:N} to false
				set {FastPlaceCheck} to "&eDesactivado"
				wait 0.1 seconds
				execute player command "/ChecksMenu"
				loop all players:
					if loop-player has permission "Nova.use":
						message "{@MPrefix} &7» &cTodos los checks fueron desactivados por &e%sender%" to loop-player
						stop
		if arg-1 is "hitreach":
			if arg-2 is not set:
				wait 0.1 seconds
				open chest with 1 rows named "{@Prefix}> &eHitReach" to player
				wait 0.1 seconds
				format slot 2 of player with Green Wool named "&aActivar" to close then run [execute player command "/novatoggle hitreach enable"]
				format slot 4 of player with Glass pane named "&bEstado: %{ReachCheck}%" to close
				format slot 6 of player with Red Wool named "&cDesactivar" to close then run [execute player command "/novatoggle hitreach disable"]
				stop
			if arg-2 is "enable":
				set {ReachCheckFFB:N} to true
				set {AutoActiveKill::N} to true
				wait 0.1 seconds
				execute player command "/ChecksMenu"
				loop all players:
					if loop-player has permission "Nova.use":
						message "{@MPrefix} &7» &e%sender% &aactivo el check &eHitReach" to loop-player
						set {ReachCheck} to "&eActivado"
						stop
			if arg-2 is "disable":
				set {ReachCheckFFB:N} to false
				set {AutoActiveKill::N} to false
				wait 0.1 seconds
				execute player command "/ChecksMenu"
				loop all players:
					if loop-player has permission "Nova.use":
						message "{@MPrefix} &7» &e%sender% &cdesactivo &ael check &eHitReach" to loop-player
						set {ReachCheck} to "&eDesactivado"
						stop
		if arg-1 is "killaura":
			if arg-2 is not set:
				wait 0.1 seconds
				open chest with 1 rows named "{@Prefix}> &eKillAura" to player
				wait 0.1 seconds
				format slot 2 of player with Green Wool named "&aActivar" to close then run [execute player command "/novatoggle killaura enable"]
				format slot 4 of player with Glass pane named "&bEstado: %{KillAuraCheck}%" to close
				format slot 6 of player with Red Wool named "&cDesactivar" to close then run [execute player command "/novatoggle killaura disable"]
				stop
			if arg-2 is "enable":
				set {KillAuraCheckFFB:N} to true
				wait 0.1 seconds
				execute player command "/ChecksMenu"
				loop all players:
					if loop-player has permission "Nova.use":
						message "{@MPrefix} &7» &e%sender% &aactivo el check &eKillAura" to loop-player
						set {KillAuraCheck} to "&eActivado"
						stop
			if arg-2 is "disable":
				set {KillAuraCheckFFB:N} to false
				wait 0.1 seconds
				execute player command "/ChecksMenu"
				loop all players:
					if loop-player has permission "Nova.use":
						message "{@MPrefix} &7» &e%sender% &cdesactivo &ael check &eKillAura" to loop-player
