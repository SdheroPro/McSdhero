# <!--! CLEAR COMMAND !--!>#
command /NovaClear [<player>] [<text>]:
	permission: Nova.use
	permission message: {@NoPermission}
	usage: /NovaClear [<player>] [<text>]
	trigger:
		if argument 1 is not set:
			message "{@Prefix} &7» &cPor favor usa: &e/NovaClear (Jugador) (Check)." to sender
			stop
		if argument 2 is not set:
			message "{@Prefix} &7» &cPor favor usa: &e/Nova HacksDetect &cy selecciona un check." to sender
			stop			
		if argument 2 is "all":
			message "{@Prefix} &7» &aSe le a reseteado: &4Todos los Checks" to sender
			clear {VLKill::%arg 1%}
			clear {VLNKB::%arg 1%}
			set {KillauraCheck::%arg 1%} to false
			clear {VLKillAura::%arg 1%}
			clear {VLSFB::%arg 1%}
			clear {VLJesus::%arg 1%}
			clear {VLNSL::%arg 1%}
			clear {fastpd::%player%}
		if argument 2 is "Killaura":
			message "{@Prefix} &7» &aSe le a reseteado: &4KillAura" to sender
			clear {KillauraCheck::%arg 1%}
			set {CheckKill::%arg 1%} to false
			clear {VLKillAura::%arg 1%}
			stop
		if argument 2 is "Reach":
			message "{@Prefix} &7» &aSe le a reseteado: &4Reach" to sender
			clear {VLKill::%arg 1%}
			stop
		if argument 2 is "AntiKB":
			message "{@Prefix} &7» &aSe le a reseteado: &4AntiKB" to sender
			clear {VLNKB::%arg 1%}
			stop
		if argument 2 is "FastBow":
			message "{@Prefix} &7» &aSe le a reseteado: &4FastBow" to sender
			clear {VLSFB::%arg 1%}
			stop	
		if argument 2 is "Jesus":
			message "{@Prefix} &7» &aSe le a reseteado: &4Jesus" to sender
			clear {VLJesus::%arg 1%}
			stop
		if argument 2 is "FastPlace":
			message "{@Prefix} &7» &aSe le a reseteado: &4FastPlace" to sender
			clear {fastpd::%player%}
			stop
		
# <!--! KICK COMMAND !--!>
command /NovaKick [<player>] [<text>] [<text>]:
	permission: Nova.Use
	permission message: {@NoPermission}
	usage: /NovaKick [<player>] [<text>] [<text>]
	trigger:
		if argument 1 is not set:
			message "{@Prefix} &7» &ePor favor usa: &a/NovaKick (Jugador) (Kill?) (Motivo)" to sender
			stop
		if argument 2 is not set:
			message "{@Prefix} &7» &eKill? ejemplo: &a/NovaKick Example &e&lYes/no &aExample" to sender
			stop
		if argument 1 is set:
			if argument 2 is "No":
				if argument 3 is not set:
					message "{@Prefix} &7» &aKickeaste a: &c%arg 1% &apor: &fSin Motivo" to sender
					strike lightning effect at arg 1
					kick arg 1 because "{@KickForCommand2}"
		if argument 1 is set:
			if argument 2 is "Yes":
				if argument 3 is not set:
					message "{@Prefix} &7» &aKickeaste a: &c%arg 1% &apor: &fSin Motivo" to sender
					kill arg 1
					strike lightning effect at arg 1
					kick arg 1 because "{@KickForCommand2}"
		if argument 1 is set:
			if argument 2 is "No":
				if argument 3 is set:
					message "{@Prefix} &7» &aKickeaste a: &c%arg 1% &apor: &f%arg 3%" to sender
					strike lightning effect at arg 1
					kick arg 1 because "{@KickForCommand1}"
		if argument 1 is set:
			if argument 2 is "Yes":
				if argument 3 is set:
					message "{@Prefix} &7» &aKickeaste a: &c%arg 1% &apor: &f%arg 3%" to sender
					kill arg 1
					strike lightning effect at arg 1
					kick arg 1 because "{@KickForCommand1}"
	
